package com.relative.pruebas;

import java.lang.Number;

rule "Politica11"
	dialect "mvel"
	ruleflow-group "Politica11"
	when
		>cliente : Cliente( vinculadoQuski == "S" )
		>variablesNegocio : VariablesNegocio( )
	then
		>variablesNegocio.setDetener( true );
